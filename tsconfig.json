{
  "compilerOptions": {
    "outDir": "./built",
    "module": "commonjs",
    "allowJs": false,
    "target": "es5",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/*"]
}
